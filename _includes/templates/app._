<div class="span9 raised summary">
    <div class="row-fluid separator">
        <div class="span12">
            <div id="applied-filters" class="lead">
                <span class="operating_unit"></span>
            </div>
            <!--
            <p class="lead">We've made available detailed information about all of our current projects. Browse the summaries, click on a filter on the right, or search through the full projects below.</p>
            -->
        </div>
    </div>
    <div class="row-fluid" id="main-container">
        <div class="span2">
            <div class="row-fluid stats">
                <div class="span12 map-btn" data-value="count">
                    <p id="total-count" class="lead"></p>
                    <p><strong>Projects</strong></p>
                </div>
            </div>
            <div class="row-fluid stats">
                <div class="span12 map-btn" data-value="sources">
                    <p id="total-donors" class="lead"></p>
                    <p><strong>Funding Sources</strong></p>
                </div>
            </div>
            <div class="row-fluid stats">
                <div class="span12 map-btn active" data-value="budget">
                    <p id="total-budget" class="lead"></p>
                    <p><strong>Budget</strong> <span class="date">2011</span></p>
                </div>
            </div>
            <div class="row-fluid stats">
                <div class="span12 map-btn" data-value="expenditure">
                    <p id="total-expenditure" class="lead"></p>
                    <p><strong>Expenditure</strong> <span class="date">2011</span></p>
                </div>
            </div>
            <div class="row-fluid stats">
                <div class="span12 map-btn" data-value="hdi">
                    <p id="hdi" class="lead"></p>
                    <p><strong>HDI</strong></p>
                </div>
            </div>
        </div>
        <div class="span10">
            <div class="row-fluid map-container">
                <div class="label visible-phone">Location</div>
                <div id="homemap" class="map"></div>
            </div>
        </div>
    </div>
    <div class="separator">
        <div class="span12">
        </div>
    </div>
    <div class='row-fluid'>
        <div class="span6">
        </div>
        <div class="span6">
            <div class="row-fluid">
                <div class="span12 chart-container" id="chart-operating_unit">
                    <div class="label">Country Offices
                        <div class="btn-operating_unit btn-group toggle" data-toggle="buttons-radio">
                            <button class="btn btn-mini active" data-facet="operating_unit">Budget</button>
                            <button class="btn btn-mini" data-facet="operating_unit">Count</button>
                        </div>
                    </div>
                    <div class="chart">
                        <div class="data"></div>
                        <div class="caption"></div>
                        <div class="placeholder lead"></div>
                    </div>
                </div>
            </div>

            <div class="row-fluid">
                <div class="span12 chart-container" id="chart-focus_area">
                    <div class="label">Focus areas
                        <div class="btn-focus_area btn-group toggle" data-toggle="buttons-radio">
                            <button class="btn btn-mini active" data-facet="focus_area">Budget</button>
                            <button class="btn btn-mini" data-facet="focus_area">Count</button>
                        </div>
                    </div>
                    <div class="chart">
                        <div class="data"></div>
                        <div class="caption"></div>
                        <div class="placeholder lead"></div>
                    </div>
                </div>
            </div>
            
            <div class="row-fluid">
                <div class="span12 chart-container" id="chart-donors">
                    <div class="label">Funding Sources
                        <div class="btn-donors btn-group toggle" data-toggle="buttons-radio">
                            <button class="btn btn-mini active hidden" data-facet="donors">Count</button>
                        </div>
                    </div>
                    <div class="chart">
                        <div class="data"></div>
                        <div class="caption"></div>
                        <div class="placeholder lead"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="separator" id="projects-heading">
        <p class="lead">Projects</p>
    </div>
    <div class="row-fluid">
        <div id="projects" class="span12">
            <div class="row-fluid">
                <div class="span12">
                    <form class="form-search">
                        <input id="projects-search" type="search" class="input-xlarge" placeholder="Search for a project...">
                        <i 
                            id="help-search"
                            class="icon-question-sign help-note"
                            data-title="Search for projects"
                            data-content='To search by project name, enter any word. To search by project ID, enter  "id: " followed by the project id number. For example: "id: 00061104"'></i>
                    </form>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <div id="project-items"></div>
                </div>
            </div>
        </div> <!--/ #projects -->
    </div>

</div> <!--/ #summary -->
<div id="filters" class="span3 hidden-phone">
    <div class="row-fluid">
        <div class="span12">
            <h3>Filter projects</h3>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <form class="form-search">
                <input id="filters-search" type="search" id="filter-field" class="input" placeholder="Find a filter...">
            </form>
        </div>
    </div>
    <div class="row-fluid">
        <div id="filter-items" class="span12">
        </div>
    </div>
</div> <!--/ #filters -->
